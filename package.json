{
  "name": "vnp_stack",
  "version": "1.3.0",
  "description": "Ultra-lightweight SPA stack (VanJS + Navigo + Puter). Features a high-performance Vite pipeline with automated Brotli/Gzip compression and optional JavaScript obfuscation.",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:min": "vite build --mode min",
    "build:obf": "vite build --mode obf",
    "build:min-obf": "vite build --mode min-obf",
    "preview": "vite preview --port 4173",
    "clean": "rimraf dist"
  },
  "keywords": [
    "vanjs",
    "navigo",
    "puter",
    "vite",
    "spa",
    "lightweight",
    "obfuscation",
    "brotli"
  ],
  "author": "Christopher Ditfort",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "navigo": "^8.11.1",
    "normalize.css": "^8.0.1",
    "vanjs-core": "^1.6.0"
  },
  "devDependencies": {
    "terser": "^5.46.0",
    "vite": "^7.3.1",
    "vite-plugin-compression2": "^2.4.0",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-javascript-obfuscator": "^3.1.0",
    "vite-plugin-sitemap": "^0.8.2"
  }
}
